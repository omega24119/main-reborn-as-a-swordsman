--egg functions
function doegg1()
  while getgenv().egg1 == true do
    local args = {
      [1] = "Hatch",
      [2] = "Draw001",
      [3] = {}
    }

    game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Pets"):WaitForChild("Re_Hatch"):FireServer(unpack(args))
    wait(0.1)
  end

  end

