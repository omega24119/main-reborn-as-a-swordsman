--button
mainTab:AddButton({
  Name = "god mode!",
  Callback = function()
      local old
      old = hookmetamethod(game, "__namecall", function(self, ...)
          local args = {...}
          local method = getnamecallmethod()

          if tostring(self) == "Re_NpcDamage" and method == "FireServer" then 
              warn("Remote has been freezed")
              return nil 
          end 

          return old(self, ...)
      end)
          print("button pressed")
    end    
})

