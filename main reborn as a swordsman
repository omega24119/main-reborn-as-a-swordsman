--chest functions
function dochest1()
while getgenv().chest1 == true do
    local args = {
      [1] = "Hatch",
      [2] = "Box001"
    }

    game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Tower"):WaitForChild("Re_Hatch"):FireServer(unpack(args))
    wait(0.1)
  end

end

